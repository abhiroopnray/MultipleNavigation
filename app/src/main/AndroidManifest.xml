<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abhiroop.multiplenavigation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MultipleNavigation">
        <activity android:name=".activity.PlayerStatusActivity"></activity>
        <activity android:name=".activity.ShowParticipantsActivity" />
        <activity android:name=".activity.SelectTeamToJoinActivity" />
        <activity android:name=".activity.BaseActivity" />
        <activity android:name=".activity.ContestActivity" />
        <activity android:name=".activity.JoinContestActivity" />
        <activity android:name=".activity.MyProfile" />
        <activity android:name=".activity.CreateTeamActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>